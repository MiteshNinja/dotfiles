{
  "languageserver": {
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh"],
      "ignoredRootPaths": ["~"]
    },
    "clojure-lsp": {
      "command": "bash",
      "args": ["-c", "clojure-lsp"],
      "filetypes": ["clojure2"],
      "rootPatterns": ["project.clj"],
      "additionalSchemes": ["jar", "zipfile"],
      "trace.server": "verbose",
      "initializationOptions": {
      }
    }
  },
  "python.venvPath": "~/.virtualenvs123",
  "python.jediEnabled": false,
  "coc.source.around.enable": true,
  "emmet.includeLanguages": {"vue-html": "html", "javascript": "javascriptreact", "htmldjango": "html"},
  "tailwindCSS.htmlLanguages": ["html", "htmldjango"],
  "html.format.wrapLineLength":93,
  "sql.database": "postgresql"
}
