{
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    }
  },
  "python.venvPath": "~/.virtualenvs",
  "coc.source.around.enable": true,
  "emmet.includeLanguages": {"vue-html": "html", "javascript": "javascriptreact", "htmldjango": "html"}
}
